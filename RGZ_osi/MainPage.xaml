                Grid.Column="2"
                HorizontalAlignment="Center"
                VerticalAlignment="Stretch"
                Margin="0,0,0,35">
                <!--Объявление размера сетки окон-->
                <Grid.RowDefinitions>
                    <RowDefinition
                        Height="*" />
                    <RowDefinition
